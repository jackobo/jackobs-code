<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:valueEditors="clr-namespace:LayoutTool.ViewModels.DynamicLayout.ValueEditors;assembly=LayoutTool.ViewModels"
                    xmlns:valueEditorsViews="clr-namespace:LayoutTool.Views.Wpf.DynamicLayout.ValueEditors">

    <DataTemplate DataType="{x:Type valueEditors:MultiSelectValueEditor}">
        <valueEditorsViews:MultiSelectValueEditorView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type valueEditors:BooleanValueEditor}">
        <valueEditorsViews:BooleanValueEditorView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type valueEditors:GenderValueEditor}">
        <valueEditorsViews:GenderValueEditorView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type valueEditors:NumericValueEditor}">
        <valueEditorsViews:NumericValueEditorView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type valueEditors:DateTimeValueEditor}">
        <valueEditorsViews:DateTimeValueEditorView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type valueEditors:ExceededRoundsPlayedValueEditor}">
        <valueEditorsViews:ExceededRoundsPlayedValueEditorView/>
    </DataTemplate>


</ResourceDictionary>